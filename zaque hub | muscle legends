    local Window = OrionLib:MakeWindow({Name = "zaque hub", HidePremium = false, SaveConfig = true, ConfigFolder = "zaqueCfg", IntroEnabled = true})

    -- Main
    local MainTab = Window:MakeTab({
	Name = "Main",
